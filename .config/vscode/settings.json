{
  "editor.autoCharacterPairs": true,
  "editor.autoSaveWhenIdle": true,
  "editor.closeAlwaysConfirmsChanges": false,
  "editor.headerFontSizeDiffs": [
    5,
    3,
    1
  ],
  "editor.indentBehavior": "never",
  "editor.searchNormalizers": {
    "\"\"": "\"",
    "''": "'",
    "[\"\"]": "\"",
    "['']": "'"
  },
  "editor.visibleLineBreakCharacter": "¬",
  "editor.visibleWhitespaceCharacter": "·",
  "editor.writingToolsBehavior": "complete",
  "extension.markeditAIWriter": {
    "customWriters": [],
    "generationOptions": {},
    "instructions": "You are a writing assistant specialized in rewriting text. Always return only the rewritten or improved version of the input while strictly preserving any Markdown formatting (including headings, lists, links, and inline styles). Do not add explanations, instructions, or commentary—output only the content itself.",
    "keyboardShortcut": "Mod-Alt-/",
    "showsTooltip": true,
    "streaming": false
  },
  "general.checksForUpdates": true,
  "general.defaultOpenDirectory": "/Users/mh/Downloads",
  "general.defaultSaveDirectory": "/Users/mh/Vault",
  "general.disableCorsRestrictions": false,
  "general.mainWindowHotKey": {
    "key": "M",
    "modifiers": [
      "Shift",
      "Command",
      "Option"
    ]
  }
}